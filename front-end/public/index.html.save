  <script>
  (function(){

    // Please read js/src/trackui.js for the API and code documentation.
    TrackUI.record({
      taskName : 'evtrack',
      debug: true,


      // Example: track all clicks and poll mouse movements at 50 ms
      //regularEvents: "click",
      //pollingEvents: "mousemove",
      //pollingMs: 50,

      // Example: pure event polling at 500 ms
    
    regularEvents: "click dblclick keydown wheel scroll mousemove",
    pollingEvents: " ",
    pollingMs: 50,
    postServer: "/save.php",
    callback : function (ev) {

      if (ev.type === `click` || ev.type === `dblclick` || ev.type === `mousemove`) {
        return {clientX : ev.clientX, clientY : ev.clientY}
      } else if (ev.type === `wheel`) {
        return {clientX : ev.clientX, clientY : ev.clientY, deltaX : ev.deltaX , deltaY : ev.deltaY , deltaZ : ev.deltaZ}
      }

      return {}
    }
    });

  })();
  </script>

